{
  "name": "express-boilerplate",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/main.ts -w src",
    "build": "vite build",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore --no-fix src test",
    "start:preview": "cross-env NODE_ENV=production nodemon ./dist/server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-transform-typescript": "^7.21.3",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.14",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-vitest": "^0.1.5",
    "eslint-plugin-vitest-globals": "^1.3.1",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.8"
  },
  "dependencies": {
    "express-beans": "^1.0.2",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  }
}
