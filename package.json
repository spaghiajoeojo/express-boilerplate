{
  "name": "express-boilerplate",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/main.ts -w src",
    "build": "vite build",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore --no-fix src test",
    "start:prod": "nodemon server.js",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitest/coverage-c8": "^0.29.3",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-vitest": "^0.0.54",
    "eslint-plugin-vitest-globals": "^1.3.1",
    "jsdom": "^21.1.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vitest": "^0.29.3",
    "vitest-mock-extended": "^1.1.1",
    "vitest-sonar-reporter": "^0.3.5"
  },
  "dependencies": {
    "nodemon": "^2.0.21",
    "express": "^4.18.2",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "ts-node": "^10.9.1"
  }
}
